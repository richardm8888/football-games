{
    "functions": {
      "api/game.ts": {
        "memory": 1024,
        "maxDuration": 10
      },
      "api/movie_game.ts": {
        "memory": 1024,
        "maxDuration": 10
      },
      "api/update-sitemap.ts": {
        "memory": 1024,
        "maxDuration": 10
      },
      "api/sitemap.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/api/sitemap.ts",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/xml"
          }
        ]
      }
    ],
    "redirects": [
      { "source": "/difficulty", "destination": "/football", "permanent": true },
      { "source": "/difficulty/easy", "destination": "/football/easy", "permanent": true },
      { "source": "/difficulty/medium", "destination": "/football/medium", "permanent": true },
      { "source": "/difficulty/hard", "destination": "/football/hard", "permanent": true }
    ],
    "devCommand": "npm start"
}
